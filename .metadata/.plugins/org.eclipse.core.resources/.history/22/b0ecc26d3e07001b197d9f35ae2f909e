		IOperation sum1 = new Addition();
		sum1.add(mult1);
		sum1.add(n3);
		
	}
	
	public void iterate(IVisitor visitor) {
		//visitor.dothis(this)
	}
	
	public void recursive_method(IOperation tree) {
		tree.;;
	}

	
}
